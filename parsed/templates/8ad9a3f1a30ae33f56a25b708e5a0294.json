{"components/common/footer":"{{#if banners.bottom}}\r\n<div class=\"banners\" data-banner-location=\"bottom\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.bottom 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n\r\n<footer id=\"footer-wrapper\">\r\n    <section id=\"main-footer\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-1\">\r\n                    <h5>Shop</h5>\r\n                    <ul>\r\n                        {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                        <!-- <li><a href=\"/lookbook/\">Lookbook</a></li> -->\r\n                    </ul>\r\n                </div>\r\n                <div class=\"col-2\">\r\n                    <h5>Customer Service</h5>\r\n                    <ul>\r\n                        <li><a href=\"/about-us/\">About Us</a></li>\r\n                        <li><a href=\"giftcertificates.php\">Gift Certificates</a></li>\r\n                        <!-- <li><a href=\"#\">Help & FAQs</a></li> -->\r\n                        <!-- <li><a href=\"#\">Advanced Search</a></li> -->\r\n                        <li><a href=\"/shipping-returns/\">Shipping & Returns</a></li>\r\n                        <li><a href=\"/privacy-policy/\">Privacy Policy</a></li>\r\n                        <li><a href=\"/contact-us/\">Contact Us</a></li>\r\n                        <li><a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a></li>\r\n                    </ul>\r\n                </div>\r\n                <div class=\"col-3\">\r\n                    {{> components/common/social-links}}\r\n                    <p>&nbsp;</p>\r\n                    {{> components/halothemes/footer-facebook-like-button}}\r\n                </div>\r\n                <div class=\"col-4\">\r\n                    {{#if settings.show_newsletter_box}} {{> components/common/subscription-form}} {{/if}}\r\n                        <div class=\"container text-{{theme_settings.copyright_alignment}}\">\r\n                            {{#if theme_settings.show_copyright_footer}}\r\n                                <div class=\"footer-copyright\">\r\n                                    <p>&copy; <span id=\"copyright_year\"></span> {{settings.store_name}} All Rights Reserved.</p>\r\n                                </div>\r\n                                <script type=\"text/javascript\">\r\n                                document.getElementById(\"copyright_year\").innerHTML = new Date().getFullYear();\r\n                                </script>\r\n                            {{/if}}\r\n            \r\n                            {{> components/common/payment-icons}}\r\n                </div>\r\n                <div class=\"col-5\">\r\n                            <table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\r\n<tr>\r\n<td width=\"135\" align=\"center\" valign=\"top\"><script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name=www.shopchrome.com&amp;size=S&amp;lang=en\"></script><br />\r\n<a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a></td>\r\n</tr>\r\n</table>\r\n                        </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</footer>\r\n","components/common/payment-icons":"<div class=\"footer-payment-icons\">\r\n    {{#if theme_settings.show_accept_visa}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_amex}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_mastercard}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_paypal}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n    {{/if}}\r\n    {{#if theme_settings.show_accept_discover}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n    {{/if}}\r\n</div>\r\n","components/common/social-links":"{{#if social_media}}\r\n    <h5>Stay Connected</h5>\r\n    <ul class=\"social-icons\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                {{#if display_name '===' 'google+'}}\r\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\r\n                    </a>\r\n                {{else}}\r\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/subscription-form":"<div id=\"footerSubcribeForm\">\r\n    <h5>GET IN THE KNOW</h5>\r\n    <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"footerSubcribeFormSubmit\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n            <input type=\"hidden\" name=\"check\" value=\"1\">\r\n            <div class=\"form-field\">\r\n                <div class=\"form-wrap\">\r\n                    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Enter your email\">\r\n                    <input class=\"btn btn-subcribe\" type=\"submit\" value=\"Continue\">\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\r\n","components/halothemes/footer-facebook-like-button":"<div class=\"facebook-like\">\r\n    <p>Like us <span>on </span> Facebook</p>\r\n    <!-- FaceBook Like Button Code -->\r\n    <div id=\"fb-root\"></div>\r\n    <script>\r\n    (function(d, s, id) {\r\n        var js, fjs = d.getElementsByTagName(s)[0];\r\n        if (d.getElementById(id)) return;\r\n        js = d.createElement(s);\r\n        js.id = id;\r\n        js.src = \"//connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v2.3\";\r\n        fjs.parentNode.insertBefore(js, fjs);\r\n    }(document, 'script', 'facebook-jssdk'));\r\n    </script>\r\n    <div class=\"fb-like fb_iframe_widget\" data-href=\"https://www.facebook.com/CHROMETX/\" data-layout=\"button_count\" data-action=\"like\" data-show-faces=\"false\" data-share=\"true\"></div>\r\n</div>\r\n"}
