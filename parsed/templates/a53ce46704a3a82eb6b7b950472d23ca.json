{"components/carousel":"<section class=\"heroCarousel owl-carousel\"\r\n    data-owl='{\r\n        \"nav\": true,\r\n        \"dots\": true,\r\n        \"items\": 1,\r\n        \"autoplay\": true,\r\n        \"autoplayTimeout\": {{carousel.swap_frequency}},\r\n        \"autoplayHoverPause\": true,\r\n        \"loop\": true,\r\n        \"autoHeight\":true,\r\n        \"responsiveRefreshRate\": 0\r\n    }'>\r\n    {{#each carousel.slides}}\r\n    <div class=\"heroCarousel-slide text-{{theme_settings.hero_carousel_content_alignment}}\" >\r\n        {{#if button_text}}\r\n        {{else}}\r\n        <a href=\"{{url}}\">\r\n        {{/if}}\r\n            <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"/>\r\n            {{#if heading}}\r\n                {{> components/carousel-content show_background=true}}\r\n            {{else}}\r\n                {{#if text}}\r\n                    {{> components/carousel-content show_background=true}}\r\n                {{else}}\r\n                    {{#if button_text}}\r\n                        {{> components/carousel-content show_background=false}}\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n        {{#if button_text}}\r\n        {{else}}\r\n        </a>\r\n        {{/if}}\r\n    </div>\r\n    {{/each}}\r\n</section>\r\n","components/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\r\n   <div class=\"heroCarousel-inner\">\r\n      <h1 class=\"heroCarousel-title animated\">{{{heading}}}</h1>\r\n      <p class=\"heroCarousel-description animated\">{{{text}}}</p>\r\n      {{#if button_text}}\r\n         <a class=\"heroCarousel-action btn btn-primary animated\" href=\"{{url}}\">{{{button_text}}} <i class=\"fa fa-caret-right\"></i></a>\r\n      {{/if}}\r\n   </div>\r\n</div>\r\n"}
