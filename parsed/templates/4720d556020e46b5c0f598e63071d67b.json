{"components/category/product-listing":"<div class=\"category-toolbar\">\r\n    <div class=\"view-as-btn\">\r\n        <div class=\"btn-group\">\r\n            <a href=\"javascript:void(0);\" id=\"grid-view\" title=\"Grid View\" class=\"{{#if theme_settings.product_list_display_mode '===' 'grid'}} current-view {{/if}}\">\r\n                <div>\r\n                    <div class=\"icon-bar\"></div>\r\n                    <div class=\"icon-bar\"></div>\r\n                    <div class=\"icon-bar\"></div>\r\n                </div>\r\n                <span>GRID</span>\r\n            </a>\r\n            <a href=\"javascript:void(0);\" id=\"list-view\" title=\"List View\" class=\"{{#if theme_settings.product_list_display_mode '===' 'list'}} current-view {{/if}}\">\r\n                <div>\r\n                    <div class=\"icon-bar\"></div>\r\n                    <div class=\"icon-bar\"></div>\r\n                    <div class=\"icon-bar\"></div>\r\n                </div>\r\n                <span>LIST</span>\r\n            </a>\r\n        </div>\r\n    </div>\r\n    {{> components/products/filter sort=pagination.category.sort}}\r\n</div>\r\n<form action=\"{{urls.compare}}\" method='POST' data-product-compare>\r\n    {{#if theme_settings.product_list_display_mode '===' 'grid'}}\r\n        {{> components/products/grid products_new1=products_new products=category.products show_compare=category.show_compare theme_settings=theme_settings columns=theme_settings.categorypage_products_column_count}}\r\n    {{else}}\r\n        {{> components/products/list products_new1=products_new products=category.products show_compare=category.show_compare theme_settings=theme_settings columns=theme_settings.categorypage_products_column_count}}\r\n    {{/if}}\r\n</form>\r\n{{> components/common/paginator pagination.category}}\r\n","components/category/sort-box":"<fieldset class=\"form-fieldset actionBar-section\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\r\n        <select class=\"form-select form-select--small\" name=\"sort\" id=\"sort\">\r\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\r\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\r\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\r\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\r\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\r\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\r\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\r\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\r\n            {{#if result_count}}\r\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\r\n            {{/if}}\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\n","components/common/paginator":"<div class=\"pagination\">\r\n    <ul class=\"pagination-list\">\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            {{#if this.number '==' ../current}}\r\n                <li class=\"pagination-item pagination-item--current\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"pagination-item\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\r\n                    {{lang 'common.next'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n\r\n    <div class=\"compare-link\">\r\n        <a href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}}<span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n    </div>\r\n</div>\r\n","components/halothemes/haloAdvancedProductImageSwap":"<div class=\"productView\">\r\n        {{#each product.customizations}}\r\n            {{{dynamicComponent 'components/products/customizations'}}}\r\n        {{/each}}\r\n    <div data-product-option-change style=\"display:none;\">\r\n        {{#each product.options}}\r\n            {{{dynamicComponent 'components/products/options'}}}\r\n        {{/each}}\r\n    </div>\r\n</div>\r\n\r\n","components/products/card":"<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\r\n    <figure class=\"card-figure\">\r\n        <div class=\"prod-image\">\r\n            <a href=\"{{url}}\">\r\n                <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)