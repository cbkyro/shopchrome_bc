{"components/common/cart-preview":"<div class=\"previewCart\">\r\n    {{#if cart.items.length}}\r\n        <div class=\"preview-cart-titles\">\r\n            <div class=\"item-info\">{{lang 'cart.checkout.item'}}</div>\r\n            <div class=\"item-price\">{{lang 'cart.checkout.price'}}</div>\r\n            <div class=\"item-qty\">{{lang 'cart.checkout.quantity'}}</div>\r\n            <div class=\"item-total\">{{lang 'cart.checkout.total'}}</div>\r\n        </div>\r\n        <ul class=\"previewCartList\">\r\n            {{#each cart.items}}\r\n                <li class=\"previewCartItem\">\r\n                    <div class=\"item-info\">\r\n                        <div class=\"previewCartItem-image\">\r\n                            {{#if type '==' 'GiftCertificate'}}\r\n                                <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\r\n                            {{else}}\r\n                                <img src=\"{{getImage image 'productthumb_size' (cdn ../../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                            {{/if}}\r\n                        </div>\r\n\r\n                        <div class=\"previewCartItem-name\">\r\n                            {{#if brand.name}}\r\n                            <span class=\"previewCartItem-brand\">\r\n                                {{brand.name}}\r\n                            </span>\r\n                            {{/if}}\r\n                            <h6><a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a></h6>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n                    <div class=\"item-price\">\r\n                        {{price.formatted}}\r\n                    </div>\r\n                    <div class=\"item-qty\">\r\n                        {{quantity}}\r\n                    </div>\r\n                    <div class=\"item-total\">\r\n                        <strong class=\"cart-item-value {{#if total_discounted}}price--discounted{{/if}}\">{{total.formatted}}</strong>\r\n                        {{#if total_discounted}}\r\n                            <strong class=\"cart-item-value\">{{total_discounted.formatted}}</strong>\r\n                        {{/if}}\r\n                    </div>\r\n\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <div class=\"preview-cart-total\">\r\n            <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value\">\r\n                <span>{{cart.sub_total.formatted}}</span>\r\n            </div>\r\n        </div>\r\n        <div class=\"preview-cart-grand-total\">\r\n             <div class=\"cart-total-label\">\r\n                <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\r\n            </div>\r\n            <div class=\"cart-total-value cart-total-grandTotal\">\r\n                <span>{{cart.grand_total.formatted}}</span>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"previewCartAction\">\r\n\r\n            <div class=\"previewCartAction-viewCart{{#unless cart.show_primary_checkout_button}} previewCartAction-viewCart--sole{{/unless}}\">\r\n                <a href=\"{{urls.cart}}\" class=\"btn btn-alt\">\r\n                    {{lang 'cart.preview.view_cart'}}\r\n                </a>\r\n            </div>\r\n\r\n            {{#if cart.show_primary_checkout_button}}\r\n                <div class=\"previewCartAction-checkout\">\r\n                    <a href=\"{{urls.checkout.single_address}}\" class=\"btn btn-checkout\">\r\n                        {{lang 'cart.preview.checkout_now'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            {{#if cart.show_multiple_address_shipping}}\r\n                <div class=\"previewCartAction-checkoutMultiple\">\r\n                    <a href=\"{{urls.checkout.multiple_address}}\">\r\n                        {{lang 'cart.preview.checkout_multiple'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            {{#if cart.additional_checkout_buttons}}\r\n                <div class=\"cart-additionalCheckoutButtons\">\r\n                    {{#each cart.additional_checkout_buttons}}\r\n                        {{{this}}}\r\n                    {{/each}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"previewCart-emptyBody\">\r\n            <div class=\"alert alert-warning text-center\">{{lang 'cart.checkout.empty_cart'}}</div>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n","components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\r\n<li id=\"currency-converter\">\r\n    <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{{currency_selector.active_currency_flag}}}<span>{{currency_selector.active_currency_code}}</span><i class=\"fa fa-caret-down\"></i></a>\r\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"triangle-with-shadow\"></div>\r\n        {{#each currency_selector.currencies}}\r\n        <li class=\"dropdown-menu-item\">\r\n            <a href=\"{{{switch_url}}}\">\r\n                {{#if is_active}}\r\n                {{{flag}}} <strong>{{name}}</strong>\r\n                {{else}}\r\n                {{{flag}}} {{name}}\r\n                {{/if}}\r\n            </a>\r\n        </li>\r\n        {{/each}}\r\n    </ul>\r\n</li>\r\n{{/if}}\r\n","components/common/header":"{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    <div class=\"banner\">\r\n        {{{limit banners.top 1}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <div class=\"header-top hide\" id=\"top-bar-promotion\">\r\n        <div class=\"container\">\r\n            <p class=\"text-center\">\r\n                <span><strong>FREE SHIPPING ON ALL ORDERS OVER $75!</strong></span>\r\n                <!-- <a href=\"/Cras-in-nunc-non-ipsum-duo-cursus-ultrices-est_p_1.html\" class=\"link\"> Shop Now</a> -->\r\n                <a href=\"javascript:void(0)\" class=\"close\">Ã—</a>\r\n            </p>\r\n        </div>\r\n    </div>\r\n    <div class=\"header-bottom\">\r\n        <div class=\"container\">\r\n            <div class=\"header-panel-top\">\r\n                <button type=\"button\" id=\"mobile-search-toggle\" class=\"visible-xs visible-sm\"><svg><use xlink:href=\"#icon-close\"></use></svg><svg><use xlink:href=\"#icon-search\"></use></svg></button>\r\n                {{> components/common/navigation}}\r\n            </div>\r\n            <div class=\"header-panel row\">\r\n                <div class=\"col-md-4 col-sm-12\">\r\n\r\n                    <div id=\"st-trigger-effects\" class=\"visible-sm visible-xs\">\r\n                        <a class=\"menu-wrapper\" data-effect=\"st-effect-1\" href=\"javascript:void(0);\">\r\n                            <!-- <svg><use xlink:href=\"#icon-trigger-bars\" /></svg>\r\n                            <svg><use xlink:href=\"#icon-close\"></use></svg> -->\r\n                            <div class=\"hamburger-menu\"></div>\r\n                        </a>\r\n                    </div>\r\n\r\n                    {{#if template_file '===' 'pages/home'}}\r\n                    <h1 class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                    </h1> {{else}}\r\n                    <div class=\"header-logo\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                    {{/if}}\r\n\r\n                    <div id=\"mobile-cart\" class=\"visible-sm visible-xs\"><a href=\"{{urls.cart}}\">\r\n                        <!-- <img class=\"icon-cart\" src=\"/assets/img/cart-4.png\" alt=\"\"> -->\r\n                        <svg class=\"icon-cart\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                        <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span></a></div>\r\n                </div>\r\n                <div class=\"col-md-8 col-sm-12 text-right\">\r\n                    <!-- <p class=\"own-text\">Free shipping on All Orders. No Minimum Purchase.</p> -->\r\n\r\n                    <div id=\"top-cart\" class=\"top-cart hidden-xs hidden-sm\">\r\n                        <a data-cart-preview href=\"javascript:void(0);\">\r\n                            <svg class=\"icon-cart\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                            <span class=\"navUser-item-cartLabel\">Shopping Cart</span><span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                        </a>\r\n                        <div id=\"cart-preview-dropdown\">\r\n                            <div class=\"triangle-with-shadow\"></div>\r\n                            <div class=\"cart-preview-inner\">\r\n                                {{> components/common/cart-preview}}\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    {{#if settings.show_wishlist}}\r\n                    <div class=\"wishlist-link hidden-xs hidden-sm\">\r\n                        <a href=\"{{urls.account.wishlists.all}}\">\r\n                        <span class=\"icon icon--heart\"><svg><use xlink:href=\"#icon-heart\" /></svg></span><span>{{lang 'account.nav.my_wish_lists'}}</span></a>\r\n                    </div>\r\n                    {{/if}}\r\n\r\n                    {{#if settings.account_creation_enabled}}\r\n                     <div class=\"login-register hidden-xs hidden-sm\">\r\n                         {{#if customer}}\r\n                           <div class=\"logged-in\">{{lang 'Welcome back,'}} <strong>{{customer.name}}. </strong><a href=\"{{urls.auth.logout}}\"><span>{{lang 'common.logout'}}</span></a></div>\r\n                         {{else}}\r\n                           <div class=\"not-logged\">\r\n                             <a href=\"{{urls.auth.login}}\"><span>{{lang 'common.login'}}</span></a> {{lang 'common.or'}} <a href=\"{{urls.auth.create_account}}\"><span>{{lang 'common.sign_up'}}</span></a>\r\n                           </div>\r\n                         {{/if}}\r\n                     </div>\r\n                 {{/if}}\r\n                </div>\r\n            </div>\r\n\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n        <div class=\"container\">\r\n            {{> components/common/navigation-menu}}\r\n        </div>\r\n    </div>\r\n\r\n\r\n    <div class=\"sticky_logo\">\r\n        <a href=\"{{settings.base_url}}\" title=\"{{settings.store_logo.title}}\"><img src=\"/product_images/uploaded_images/logo-scroll.png\" alt=\"\"></a>\r\n    </div>\r\n\r\n</header>\r\n","components/common/navigation":"<nav class=\"navUser\">\r\n    <ul>\r\n        <li class=\"hidden-xs hidden-sm\">{{> components/halothemes/header-phone-number}}</li>\r\n        {{#if customer.store_credit.value '>' 0}}\r\n        <li class=\"hidden-xs hidden-sm\">\r\n            <a class=\"navUser-action--storeCredit\" data-dropdown=\"storeCredit-dropdown\" data-options=\"align:bottom\" href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                <div class=\"triangle-with-shadow\"></div>\r\n                {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n            </div>\r\n        </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"hidden-xs hidden-sm\">\r\n            <a href=\"{{urls.gift_certificate.purchase}}\"><span>{{lang 'common.gift_cert'}}</span></a>\r\n        </li>\r\n        {{/if}}\r\n\r\n        {{#if customer}}\r\n        <li id=\"accountDropdown\" class=\"hidden-xs hidden-sm\">\r\n            <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"accountSelection\" aria-controls=\"accountSelection\" aria-expanded=\"false\"><span>{{lang 'common.account'}}</span><i class=\"fa fa-caret-down\"></i></a>\r\n            <ul class=\"dropdown-menu\" id=\"accountSelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n                <div class=\"triangle-with-shadow\"></div>\r\n                <li><a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a></li>\r\n                {{#if settings.returns_enabled}}\r\n                <li><a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a></li>\r\n                {{/if}}\r\n                <li><a href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a></li>\r\n                <li><a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a></li>\r\n                <li><a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a></li>\r\n                <li><a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a></li>\r\n            </ul>\r\n        </li>\r\n        {{else}}\r\n        <li class=\"hidden-xs hidden-sm\"><a href=\"{{urls.account.index}}\"><span>{{lang 'common.account'}}</span></a></li>\r\n        {{/if}}\r\n\r\n        {{#if customer}}\r\n        <li id=\"mobileAccountSidebar\" class=\"hidden-md hidden-lg\"><a data-effect=\"st-effect-2\" class=\"navUser-action\" href=\"#\">\r\n            <!-- <i class=\"fa fa-user\"></i> -->\r\n            <svg class=\"icon-users\"><use xlink:href=\"#icon-users\"></use></svg>\r\n            <span>{{lang 'common.account'}}</span></a>\r\n        </li>\r\n        <!-- <li id=\"mobileAccountSidebar\" class=\"hidden-md hidden-lg\"><a data-effect=\"st-effect-2\" class=\"navUser-action\" href=\"#\">\r\n            <img src=\"/assets/img/gender_neutral_user1600.png\" alt=\"\"><span>{{lang 'common.account'}}</span></a></li> -->\r\n        {{else}}\r\n        <li id=\"mobileAccountSidebar\" class=\"hidden-md hidden-lg\"><a data-effect=\"st-effect-2\" href=\"{{urls.account.index}}\">\r\n            <!-- <i class=\"fa fa-user\"></i> -->\r\n            <svg class=\"icon-users\"><use xlink:href=\"#icon-users\"></use></svg>\r\n            <span>{{lang 'common.account'}}</span></a>\r\n        </li>\r\n        <!-- <li id=\"mobileAccountSidebar\" class=\"hidden-md hidden-lg\"><a data-effect=\"st-effect-2\" href=\"{{urls.account.index}}\"><img src=\"/assets/img/gender_neutral_user1600.png\" alt=\"\"><span>{{lang 'common.account'}}</span></a></li> -->\r\n        {{/if}}\r\n\r\n\r\n\r\n        {{> components/common/currency-selector}}\r\n\r\n        <li id=\"quickSearch\">\r\n            {{> components/common/quick-search}}\r\n        </li>\r\n\r\n    </ul>\r\n</nav>\r\n","components/common/navigation-list":"{{#if children}}\r\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\">\r\n    {{name}}\r\n</a>\r\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu\" href=\"{{url}}\">\r\n                        {{name}}\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        <li class=\"close-navPage-childList\">\r\n                            <svg><use xlink:href=\"#icon-close\"></use></svg>\r\n                        </li>\r\n                        {{#each children}}\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                {{else}}\r\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n</div>\r\n{{else}}\r\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n{{/if}}\r\n","components/common/navigation-menu":"<nav class=\"navPages\">\r\n    <ul class=\"navPages-list\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                 <li class=\"navPages-item\">\r\n                     <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\r\n                 </li>\r\n             {{/each}}\r\n        {{/unless}}\r\n        {{#if settings.show_wishlist}}\r\n            <li class=\"navPages-item wishlist-link show_mobile\">\r\n                <a href=\"{{urls.account.wishlists.all}}\">\r\n                    <span class=\"icon icon--heart\"><svg><use xlink:href=\"#icon-heart-2\" /></svg></span>\r\n                    <span>{{lang 'account.nav.my_wish_lists'}}</span></a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item gift-certificates-link show_mobile\">\r\n                <a href=\"{{urls.gift_certificate.purchase}}\">\r\n                    <span class=\"icon icon--gift\"><svg><use xlink:href=\"#icon-gift\" /></svg></span>\r\n                    <span>{{lang 'common.gift_cert'}}</span></a>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navPages-item hotline show_mobile\">\r\n            <span class=\"icon icon--phone\"><svg><use xlink:href=\"#icon-phone\" /></svg></span>\r\n            {{> components/halothemes/header-phone-number}}\r\n        </li>\r\n        {{> components/common/currency-selector}}\r\n    </ul>\r\n</nav>\r\n","components/common/quick-search":"{{{ snippet \"forms_search\"}}}\r\n<button class=\"search-toggle hide\"><svg><use xlink:href=\"#icon-close\"></use></svg><svg><use xlink:href=\"#icon-search\"></use></svg></button>\r\n<form class=\"form\" action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button type=\"submit\"><svg><use xlink:href=\"#icon-search\"></use></svg></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n<div class=\"quickSearchResultsWrap\" data-prevent-quick-search-close>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\r\n    {{#if settings.store_logo.image}}\r\n        <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/halothemes/header-phone-number":"Order online or call us <a href=\"tel:{{settings.phone_number}}\"><span>{{settings.phone_number}}</span></a>\n"}
